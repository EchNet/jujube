{
	service: {
		serviceModules: [{
			"$class": "net.ech.service.GetResourceServiceModule",
			method: "GET",
			path: "/dwarf",
			resource: {
				"$class": "net.ech.mongo.MongoCollectionResource",
				collectionName: "dwarves",
				mongoDatabase: {
					"$class": "net.ech.mongo.MongoDatabase",
					uri: "mongodb://localhost:27027/test"
				}
			}
		},{
			"$class": "net.ech.service.GetResourceServiceModule",
			method: "GET",
			path: "/",
			resource: {
				"$class": "net.ech.nio.UrlResource",
				base: "http://ech.net/"
			}
		}],
		postProcessors: [{
			"$class": "net.ech.service.AccessControlPostProcessor"
		},{
			"$class": "net.ech.service.CacheControlPostProcessor"
		}]
	}
}
