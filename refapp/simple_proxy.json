{
	service: {
		modules: [{
			method: "GET",
			path: "/meregaer",
			module: {
				"$class": "net.ech.service.GetResourceServiceModule",
				resource: {
					"$class": "net.ech.nio.UrlResource",
					base: "http://ech.net/meregaer/"
				}
			}
		},{
			method: "GET",
			path: "/dwarf",
			module: {
				"$class": "net.ech.service.GetResourceServiceModule",
				resource: {
					"$class": "net.ech.mongo.MongoCollectionResource",
					collectionName: "dwarves",
					mongoDatabase: {
						"$class": "net.ech.mongo.MongoDatabase",
						uri: "mongodb://localhost:27027/test"
					}
				}
			}
		},{
			module: {
				"$class": "net.ech.service.AccessControlServiceModule"
			}
		},{
			method: "GET",
			module: {
				"$class": "net.ech.service.CacheControlServiceModule"
			}
		}]
	}
}
